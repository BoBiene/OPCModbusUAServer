{
    "configurations": [
        {
            "name": "Node.js Launch",
            "program": "${workspaceFolder}/server.js",
            "args": ["-p", "8081", "--modbus-holdingregister", "1:5", "10", "50:20", "--modbus-holdingregister","100:5", "--modbus-host", "localhost"],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Docker Node.js Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "platform": "node"
        }
    ]
}